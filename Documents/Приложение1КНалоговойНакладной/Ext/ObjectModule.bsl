	Макет.КодЯзыкаМакета = "ru";
		
	СписокДополнительныхПараметров = "ДоговорНаименованиеДляПечати,";
	МассивСтруктурСтрок = ФормированиеПечатныхФорм.ДополнительнаяИнформация(Шапка, СписокДополнительныхПараметров, "uk");
