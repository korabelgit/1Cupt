		ДоговорНаименованиеДляПечати = МассивСтруктурСтрок[0].ЗначениеПараметра;
	Иначе	
    	ДоговорНаименованиеДляПечати = "";
	КонецЕсли;
	
	// кто выписал налоговую накладную нужно формировать в виде "И.О. Фамилия"
